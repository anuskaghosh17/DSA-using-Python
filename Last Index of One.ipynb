    "#last occurance of one\n",
    "import numpy as np\n",
    "x = np.array([0,1,1,0,1,0,1,0,0])\n",
    "def last1_occur(x):\n",
    "    index = -1\n",
    "    n = len(x)\n",
    "    for i in range(n):\n",
    "        if x[i]==1:\n",
    "            index = i\n",
    "    return index\n",
    "print(\"last index of one:\", last1_occur(x))"
   ]
  }
