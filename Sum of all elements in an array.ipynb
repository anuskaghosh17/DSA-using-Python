   "source": [
    "#Write a function to calculate the sum of all elements in an Array\n",
    "import numpy as np\n",
    "x = np.array([10,20,30,40,50])\n",
    "def sum(x):\n",
    "    total_sum = 0\n",
    "    for i in x:\n",
    "        total_sum += i\n",
    "    return total_sum\n",
    "sum(x)"
   ]
